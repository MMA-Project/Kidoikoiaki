{
  "$schema": "https://raw.githubusercontent.com/Azure/bicep/main/src/bicep/schemas/bicepconfig.schema.json",
  "analyzers": {
    "core": {
      "enabled": true,
      "rules": {
        "no-hardcodedLocations": {
          "level": "warning"
        },
        "no-hardcodedEnvironmentUrls": {
          "level": "warning"
        },
        "no-unused-parameters": {
          "level": "warning"
        },
        "no-unused-variables": {
          "level": "warning"
        },
        "use-symbolic-reference-for-latest-api-versions": {
          "level": "warning"
        }
      }
    }
  }
}
